<filesystem>

    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="de-foopara-phpcsmd-annotation-phpcs-warning.xml" url="nbresloc:/de/foopara/phpcsmd/resources/phpcs/warning.xml"/>
            <file name="de-foopara-phpcsmd-annotation-phpcs-error.xml" url="nbresloc:/de/foopara/phpcsmd/resources/phpcs/error.xml"/>

            <file name="de-foopara-phpcsmd-annotation-phpcs-comment.xml"     url="nbresloc:/de/foopara/phpcsmd/resources/phpcs/comment.xml"/>
            <file name="de-foopara-phpcsmd-annotation-phpcs-forbidden.xml"   url="nbresloc:/de/foopara/phpcsmd/resources/phpcs/forbidden.xml"/>
            <file name="de-foopara-phpcsmd-annotation-phpcs-unreachable.xml" url="nbresloc:/de/foopara/phpcsmd/resources/phpcs/unreachable.xml"/>

            <file name="de-foopara-phpcsmd-annotation-phpmd-violation.xml" url="nbresloc:/de/foopara/phpcsmd/resources/phpmd/violation.xml"/>
            <file name="de-foopara-phpcsmd-annotation-phpcpd-violation.xml" url="nbresloc:/de/foopara/phpcsmd/resources/phpcpd/violation.xml"/>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="org-netbeans-modules-php-project-ui-options-PHPOptionsCategory">
            <file name="PHPCSMD.instance">
                <attr methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory" name="instanceCreate"/>
                <attr name="controller" newvalue="de.foopara.phpcsmd.ui.PanelController"/>
                <attr bundlevalue="de.foopara.phpcsmd.Bundle#AdvancedOption_DisplayName_PHPCSMD" name="displayName"/>
                <attr intvalue="250" name="position"/>
            </file>
        </folder>
    </folder>

    <folder name="TaskList">
        <folder name="Scanners">
            <file name="ViolationReporter.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.PushTaskScanner"/>
                <attr name="instanceClass" stringvalue="de.foopara.phpcsmd.ViolationReporter"/>
            </file>
        </folder>
    </folder>

</filesystem>
